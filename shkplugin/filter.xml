<root>
    <table name="bildung_gesamt">
        <column name="Korrektur" />
        <column name="Unterbereich">
            <value name="Heime">
                <column name="Angebotsform" />
            </value>
            <value name="Kita">
                <column name="Angebotsform" />
            </value>
            <value name="Schule">
                <column name="Angebotsform">
                    <value name="*">
                        <column name="Schuelerzahl_2016" alias="Sch&#252;lerzahlen" input="range"/>
                    </value>
                    <value name="Grundschule">
                        <column name="FILTER_Ganztagsbetreuungsangebot" alias="Ganztagsbetreuung" />
                    </value>
                </column>
            </value>
        </column>
    </table>
    <table name="gesundheit_gesamt">
        <column name="Korrektur" />
        <column name="Unterbereich">
            <value name="Hausarzt">
                <column name="Angebotsform">
                    <value name="*">
                        <column name="FILTER_Alter_GROB" alias="Alter"/>
                        <column name="FILTER_Praxiskooperation_GROB" alias="Praxiskooperation"/>
                    </value>
                </column>
            </value>
            <value name="Facharzt">
                <column name="Angebotsform">
                    <value name="*">
                        <column name="FILTER_Alter_GROB" alias="Alter"/>
                        <column name="FILTER_Praxiskooperation_GROB" alias="Praxiskooperation"/>
                    </value>
                </column>
            </value>
        </column>
    </table>
    <table name="nahversorgung_gesamt">
        <column name="Korrektur" />
        <column name="Unterbereich">
            <value name="Nahversorger">
                <column name="FILTER_FLAECHE" alias='Fl&#228;che (m&#178;)' />
            </value>
        </column>
    </table>
</root>